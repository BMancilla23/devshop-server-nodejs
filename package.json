{
  "name": "store-api",
  "version": "1.0.0",
  "main": "index.js",
  "_moduleAliases": {
    "@db": "dist/db",
    "@helpers": "dist/helpers",
    "@services": "dist/services",
    "@types": "dist/types",
    "@middlewares": "dist/middlewares",
    "@routes": "dist/routes",
    "@utils": "dist/utils",
    "@validations": "dist/validations",
    "@schemas": "dist/db/schemas"
  },
  "type": "module",
  "scripts": {
    "start": "node --env-file=.env dist/index.js",
    "dev": "node --import=tsx --watch --env-file=.env src/index.ts",
    "build": "tsc && tsc-alias",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "author": "Bryan Mancilla",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "argon2": "^0.41.1",
    "drizzle-orm": "^0.35.2",
    "drizzle-zod": "^0.5.1",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/module-alias": "^2.0.4",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.26.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
